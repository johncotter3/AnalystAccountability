extends layout

block content

  script.
    $('#analysts_li'.addClass('active');
  .container
    .row-fluid.heading
      .span12
        h1(style='padding-bottom: 10px;') #{title} Accuracy
        table(id="firmTable" align="center" width="100%" cellpadding="0" cellspacing="0" border="0" class="table-striped")
          thead
            tr
              th
                h3 Date
              th
                h3 Symbol
              th
                h3 Company
              th
                h3 Rating
              th
                h3 Projected Price
              th
                h3 Actual Price
              th
                h3 Percent Error
          tbody      
            each doc, i in forecast
              tr
                td
                  p #{doc.Date.getMonth()+1}/#{doc.Date.getDate()}/#{doc.Date.getFullYear()}
                td
                  p #{doc.Symbol}
                td
                  p #{doc.Company}
                td
                  p !{doc.Rating}
                td
                  p #{doc.Price_int}
                td
                  p #{doc.Actual}
                td
                  p #{doc.Percent_Diff}
